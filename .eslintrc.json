{
	"env": {
		"browser": true,
		"greasemonkey": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"dot-notation": "error",
		"eqeqeq": "error",
		"indent": [
			"error",
			"tab"
		],
		"max-len": [
			"error",
			{
				"code": 140
			}
		],
		"no-empty-function": "warn",
		"no-extra-parens": "error",
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"strict": "error"
	}
}
